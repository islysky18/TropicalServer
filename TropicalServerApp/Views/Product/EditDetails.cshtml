
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>EditDetails</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @*<link href="~/Content/TropicalStyles/ForgetPassword.css" rel="stylesheet" />*@
    <link rel="stylesheet" type="text/css" href="~/Content/TropicalStyles/Login.css">


</head>

<body>

    <div class="container">

        <div class="main">

            <div class="imageDisplay">
                <img height="100" src="@Url.Content("~/Images/Header.jpg")" />
            </div>

            <h1>EditDetails</h1>
            <form action="#">
                <div>
                    <label id="useridlbl">Tracking #:</label><input type="text" id="useridtextbox" />
                    <br />
                    <label id="useridlbl">Order Date:</label> <input type="date" id="useridtextbox" />
                    <br />
                    <label id="useridlbl">Customer ID:</label><input type="text" id="useridtextbox" />
                    <br />
                    <label id="useridlbl">Customer Name:</label><input type="text" id="useridtextbox" />
                    <br />
                    <label id="useridlbl">Address:</label> <input type="text" id="useridtextbox" />
                    <br />
                    <label id="useridlbl">Route:</label> <input type="text" id="useridtextbox" />
                    <br />
                    <button id="LoginBtn_1" type="submit" value="Submit" style="font-size:larger; float:right">Submit</button>
                    <br />
                </div>
            </form>

            <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
            </footer>
        </div>

    </div>





    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

</body>

</html>